{
  "name": "@nimel/directorr-repository",
  "private": true,
  "version": "1.0.0-rc.3",
  "author": "Nikita Melnikov",
  "license": "MIT",
  "homepage": "https://github.com/nikitaMe1nikov/directorr-repository#readme",
  "bugs": {
    "url": "https://github.com/nikitaMe1nikov/directorr-repository/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nikitaMe1nikov/directorr-repository.git"
  },
  "scripts": {
    "local-publish": "npx oao all --tree 'yarn run local-publish'",
    "build:all": "npx oao all --tree 'yarn run build'",
    "test:all": "npx oao all --tree 'yarn run test:all'",
    "lint:all": "npx oao all --tree 'yarn run lint'",
    "test-coverage:all": "npx oao all --tree 'yarn run test-coverage'",
    "prerelease": "yarn test:all && yarn build:all && npx oao prepublish",
    "release": "npx oao publish",
    "xxl": "xxl --src packages --exclude node_modules",
    "dedup": "npx yarn-deduplicate -s fewer yarn.lock"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run lint:all"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@testing-library/react": "^11.0.2",
    "@testing-library/react-hooks": "^3.4.1",
    "@types/core-js": "^2.5.3",
    "@types/enzyme": "^3.10.6",
    "@types/jest": "^26.0.13",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-jest": "^26.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "fs-extra": "^8.1.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^10.0.7",
    "oao": "^2.0.0",
    "prettier": "^2.1.1",
    "redux": "^4.0.5",
    "rollup": "^1.31.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-terser": "^5.2.0",
    "ts-jest": "^26.3.0",
    "typescript": "3.9.7",
    "xxl": "^1.3.0",
    "yarn-deduplicate": "^3.0.0"
  }
}
