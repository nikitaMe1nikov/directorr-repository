{
  "name": "@nimel/directorr-react-router",
  "version": "1.0.0",
  "keywords": [
    "directorr",
    "react",
    "router"
  ],
  "description": "Router for Reactjs using directorr",
  "main": "build/index.js",
  "umd:main": "build/index.umd.js",
  "module": "build/index.module.js",
  "browser": {
    "./build/index.js": "./build/index.js",
    "./build/index.module.js": "./build/index.module.js"
  },
  "unpkg": "build/index.umd.min.js",
  "jsnext:main": "build/index.module.js",
  "react-native": "build/index.module.js",
  "es2015": "build/index.es6.js",
  "typings": "build/index.d.ts",
  "scripts": {
    "local-publish": "node ../../scripts/build.js && yalc publish",
    "check-ts": "tsc --noEmit",
    "lint": "eslint \"./src/**/*.{tsx,ts}\" --fix",
    "test:all": "yarn run check-ts && yarn run lint"
  },
  "peerDependencies": {
    "@nimel/directorr": "1.0.0",
    "react": ">=16.10.0",
    "react-dom": ">=16.10.0"
  },
  "dependencies": {
    "@nimel/directorr-react": "1.0.0",
    "@nimel/directorr-router": "1.0.0"
  },
  "devDependencies": {
    "@nimel/directorr": "1.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
