{
  "name": "@nimel/directorr-form",
  "version": "1.0.0-rc.1",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "directorr",
    "form"
  ],
  "description": "Form logic in directorr",
  "main": "build/index.js",
  "umd:main": "build/index.umd.js",
  "module": "build/index.module.js",
  "browser": {
    "./build/index.js": "./build/index.js",
    "./build/index.module.js": "./build/index.module.js"
  },
  "unpkg": "build/index.umd.min.js",
  "jsnext:main": "build/index.module.js",
  "react-native": "build/index.module.js",
  "es2015": "build/index.es6.js",
  "typings": "build/index.d.ts",
  "scripts": {
    "local-publish": "node ../../scripts/build.js && yalc publish",
    "build": "node ../../scripts/build.js",
    "check-ts": "tsc --noEmit",
    "lint": "eslint \"./**/*.{tsx,ts}\" --fix",
    "test:all": "yarn run check-ts && yarn run lint"
  },
  "peerDependencies": {
    "@nimel/directorr": "^1.0.0-rc.1",
    "mobx": ">=5.15.6",
    "yup": ">=0.29.3"
  },
  "dependencies": {},
  "devDependencies": {
    "@nimel/directorr": "^1.0.0-rc.1",
    "@types/yup": "^0.29.6",
    "mobx": "^5.15.6",
    "yup": "^0.29.3"
  },
  "author": "Nikita Melnikov",
  "license": "MIT",
  "homepage": "https://github.com/nikitaMe1nikov/director-repo#readme",
  "bugs": {
    "url": "https://github.com/nikitaMe1nikov/director-repo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nikitaMe1nikov/director-repo.git"
  }
